<h2>Daftar Skill Pengguna</h2>
<table border="1" cellpadding="6">
  <thead>
    <tr>
      <th>Nama Pengguna</th>
      <th>Skill</th>
    </tr>
  </thead>
  <tbody>
    {% for skill in userSkills %}
      <tr>
        <td>{{ skill.full_name }}</td>
        <td>{{ skill.skill_name }}</td>
      </tr>
    {% else %}
      <tr>
        <td colspan="2">Tidak ada data.</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
